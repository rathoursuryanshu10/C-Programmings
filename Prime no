#include<stdio.h>
int main()
{
	int i,n,fact=0;
	printf("Enter the number to heck prime or not :  ");
	scanf("%d",&n);
	for(i=2;i<n;i++)
	{
		if(n%i==0)
		{
			fact=fact+1;
			break;
		}
	}
		if(fact>=1)
		{
			printf("%d is not a prime no.",n);
		}
		else
		{
			printf("%d is a prime no ",n);
		}
	return 0;
}
