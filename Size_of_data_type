#include<stdio.h>
int main()
{
	int a;
	float f;
	double d;
	char c;
	long l;
	printf(" size of int : %d byte \n",sizeof(a));
	printf(" size of float : %d byte \n",sizeof(f));
	printf(" size of double : %d byte \n",sizeof(d));
	printf(" size of char : %d byte \n",sizeof(c));
	printf(" size of long : %d byte ",sizeof(l));
}
